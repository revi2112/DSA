class Solution:
    def maxArea(self, height: List[int]) -> int:
        '''
            find area of large rectange
        '''
        start = 0
        end = len(height) - 1
        max_area = float(-inf)

        while(start < end):
            max_area = max(((end-start)* min(height[start], height[end]), max_area) )

            if height[start] < height[end]:
                start+=1
            else:
                end -=1    

        return max_area 
